# Dummy script does not need a man page. Original man page still available.
anon-ws-disable-stacked-tor: binary-without-manpage usr/sbin/tor.anondist

# Just shipping the init script, so other init script that depend on
# "Provides: tor" do not break.
anon-ws-disable-stacked-tor: init.d-script-does-not-provide-itself etc/init.d/tor.anondist

# lintian does not know, that dh_configpackage will handle that.
anon-ws-disable-stacked-tor: init.d-script-not-marked-as-conffile etc/init.d/tor
anon-ws-disable-stacked-tor: init.d-script-not-included-in-package etc/init.d/tor
anon-ws-disable-stacked-tor: script-in-etc-init.d-not-registered-via-update-rc.d etc/init.d/tor.anondist
